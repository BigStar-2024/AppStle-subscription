                               uniqueColumns="shop, jhi_key"/>
    </changeSet>

    <changeSet id="20220813142800-3" author="aqeel">
        <dropColumn tableName="shop_label">
            <column name="jhi_key" type="varchar(1000)">
                <constraints nullable="false" />
            </column>
        </dropColumn>
    </changeSet>
</databaseChangeLog>
