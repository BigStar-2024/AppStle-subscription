        <createTable tableName="selling_plan_member_info">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="shop" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="subscription_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="selling_plan_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="enable_member_inclusive_tag" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="member_inclusive_tags" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="enable_member_exclusive_tag" type="boolean">
                <constraints nullable="true" />
            </column>
            <column name="member_exclusive_tags" type="varchar(255)">
